{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts"]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name": false,
    "no-reference": false,
    "no-console": false,
    "no-empty": false,
    "member-access": false,
    "curly": false,
    "max-classes-per-file": false,
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ]
  }
}